package kodlamaio.devs.demo.entities;

import jakarta.persistence.Entity;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@NoArgsConstructor
@AllArgsConstructor
@Entity
@Table(name="Technologies")
public class LanguageTechnology { //one to many
	private int id;
	private String name;

}
